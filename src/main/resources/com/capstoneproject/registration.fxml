<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400" prefWidth="437.0" spacing="10" style="-fx-alignment: center; -fx-padding: 20;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.capstoneproject.RegistrationController">
    <children>
        <TextField fx:id="usernameField" promptText="Username" />
        <PasswordField fx:id="passwordField" promptText="Password" />
        <TextField fx:id="firstNameField" promptText="First Name" />
        <TextField fx:id="lastNameField" promptText="Last Name" />
        <TextField fx:id="emailField" promptText="Email" />
        <TextField fx:id="phoneNumberField" promptText="Phone Number" />
        <TextField fx:id="addressField" promptText="Address" />
        <TextField fx:id="ageField" promptText="Age" />
        <Button onAction="#registerButtonClicked" text="Register" />
        <Button onAction="#goBackButtonClicked" text="Go Back" />
    </children>
</VBox>
